<div class="card">
  <div class="card-photo">
    <%= cl_image_tag @artist.photo, height: 300, width: 400, crop: :fill %>
  </div>
  <div class="card-category">ARTISTE</div>
  <div class="card-header">
    <h2><%= @artist.name %></h2>
  </div>
  <div class="card-description">
    <p><%= @artist.description %></p>
  </div>
  <a class="card-link" href="#" ></a>
</div>
